    "ai": "4.3.19",
    "better-auth": "1.3.2",
    "drizzle-orm": "0.44.3",
    "hono": "4.8.5",
    "lodash": "4.17.21",
    "pg": "8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.7.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
