{
  "name": "quiz-feed",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "local": "cross-env PV_NODE_ENV=local concurrently \"tsx watch ./src/server.ts\" \"vite\"",
    "dev": "cross-env PV_NODE_ENV=dev concurrently \"tsx watch ./src/server.ts\" \"vite build --watch\"",
    "prod": "cross-env-shell PV_NODE_ENV=prod \"vite build && tsx ./src/server.ts\"",
    "eval": "tsx ./src/evals/flack-eval-dual-mode.ts",
    "dkgen": "drizzle-kit generate",
    "dkmig": "drizzle-kit migrate",
    "dumptopic": "tsx ./src/utils.ts dump",
    "loadtopic": "tsx ./src/utils.ts load",
    "loadtopics": "tsx ./src/utils.ts load",
    "cert": "mkdir -p .cert && mkcert -key-file .cert/key.pem -cert-file .cert/cert.pem localhost",
    "lint": "tsc --noEmit && eslint . --cache",
    "postinstall": "./setup-git.sh"
  },
  "dependencies": {
    "@hono/node-server": "1.18.1",
    "@hono/zod-validator": "0.7.2",
    "@js-temporal/polyfill": "0.5.1",
    "@openai/agents": "0.0.17",
    "@openai/agents-core": "0.0.17",
    "@openrouter/ai-sdk-provider": "0.7.2",
    "@slack/bolt": "4.4.0",
    "@slack/web-api": "7.9.3",
    "ai": "4.3.19",
    "better-auth": "1.3.7",
    "cron": "4.3.3",
    "drizzle-orm": "0.44.3",
    "googleapis": "155.0.0",
    "hono": "4.9.1",
    "langfuse": "3.38.4",
    "openai": "5.15.0",
    "pg": "8.16.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.0",
    "rrule-temporal": "1.1.8",
    "tailwindcss": "4.1.11",
    "tsx": "4.20.3",
    "vite": "7.1.2",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "9.31.0",
    "@slack/types": "2.15.0",
    "@stylistic/eslint-plugin": "5.2.3",
    "@tailwindcss/vite": "4.1.11",
    "@types/pg": "8.15.4",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.0",
    "concurrently": "9.2.0",
    "cross-env": "10.0.0",
    "drizzle-kit": "0.31.4",
    "eslint": "9.31.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0"
  }
}
